using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
///FitMonitor 的摘要说明
/// </summary>
public class FitMonitor
{
	public FitMonitor()
	{
		//
		//TODO: 在此处添加构造函数逻辑
		//
	}
    //记录ID
    private string id;
    //租户
    private string lessee;
    //楼宇号
    private string buildingName;
    //房间号
    private string room;
    //装修检查时间
    private string checkTime;
    //检查出的主要问题
    private string detail;
    //物业员工号
    private string employeId;
    //用于储存租户违规装修的照片的路径
    private string picturePath;
    //检查是否通过
    private Boolean isPassed;
    //该记录真删除？
    private Boolean isDeleted;
    //申请装修日期
    private string applyMaintain;

    public string Id
    {
        get { return id; }
        set { id = value;  }
    }

    public string BuildingName
    {
        get { return buildingName; }
        set { buildingName = value; }
    }

    public string Room
    { 
        get { return room; }
        set { room = value; }
    }

    public string Lessee
    {
        get { return lessee; }
        set { lessee = value; }
    }

    public string CheckTime
    {
        get { return checkTime; }
        set { checkTime = value; }
    }

    public string Detail
    {
        get { return detail; }
        set { detail = value; }
    }

    public string EmployeId
    {
        get { return employeId; }
        set { employeId = value; }
    }

    public string PicturePath
    {
        get { return picturePath; }
        set { picturePath = value; }

    }
    public Boolean IsPassed
    {
        get { return isPassed; }
        set { isPassed = value; }
    }

    public Boolean IsDeleted
    {
        get { return isDeleted; }
        set { isDeleted = value; }
    }

    public string ApplyMaintain
    {
        get { return applyMaintain; }
        set { applyMaintain = value; }
    }
}