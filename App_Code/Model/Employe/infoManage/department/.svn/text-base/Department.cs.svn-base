using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
///Department 的摘要说明
/// </summary>


namespace CEMIS.Model.Employe
{
    public class Department
    {
        public Department()
        {
            //
            //TODO: 在此处添加构造函数逻辑
            //
        }

        //部门名称
        private string name;
        public string Name
        {
            get { return name; }
            set { name = value; }
        }

        //部门主管
        private string manager;
        public string Manager
        {
            get { return manager; }
            set { manager = value; }
        }

        //部门地址
        private string address;
        public string Address
        {
            get { return address; }
            set { address = value; }
        }

        //如果有重复的部门名称，可以使用ID进行查询
        private string id;
        public string ID
        {
            get { return id; }
            set { id = value; }
        }
    }
}


