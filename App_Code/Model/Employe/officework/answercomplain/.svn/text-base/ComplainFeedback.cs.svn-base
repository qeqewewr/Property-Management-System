using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
///ComplainFeedback 的摘要说明
/// </summary>
public class ComplainFeedback
{
	public ComplainFeedback()
	{
		//
		//TODO: 在此处添加构造函数逻辑
		//
	}

    //记录ID
    private string id;
    //租户
    private string lessee;
    //楼宇号
    private string buildingName;
    //房间号
    private string room;
    //投诉内容
    private string complainContent;
    //投诉日期和时间
    private string complainDateTime;
    //联系人
    private string director;
    //联系人号码
    private string directorPhone;
    //反馈单 图片路径
    private string picturePath;
    //反馈日期和时间
    private string dealDateTime;
    //反馈内容
    private string dealContent;
    //是否处理了
    private int isDeal;

    public string Id
    {
        get { return id; }
        set { id = value; }
    }

    public string BuildingName
    {
        get { return buildingName; }
        set { buildingName = value; }
    }

    public string Lessee
    {
        get { return lessee; }
        set { lessee = value; }
    }

    public string Room
    {
        get { return room; }
        set { room = value; }
    }

    public string ComplainDateTime
    {
        get { return complainDateTime; }
        set { complainDateTime = value; }
    }
    public string ComplainContent
    {
        get { return complainContent; }
        set { complainContent = value; }
    }

    public string Director
    {
        get { return director; }
        set { director = value; }
    }

    public string DirectorPhone
    {
        get { return directorPhone; }
        set { directorPhone = value; }
    }

    public string PicturePath
    {
        get { return picturePath; }
        set { picturePath = value; }
    }

    public string DealDateTime
    {
        get { return dealDateTime; }
        set { dealDateTime = value; }
    }

    public string DealContent
    {
        get { return dealContent; }
        set { dealContent = value; }
    }
    public int IsDeal
    {
        get { return isDeal; }
        set { isDeal = value; }
    }
}