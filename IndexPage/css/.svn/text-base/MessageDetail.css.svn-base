}
.pages, .pageback {
    float: right;
    padding: 2px 0;
}
.pages a, .pages strong, .pageback a {
    border: 1px solid;
    float: left;
    height: 26px;
    line-height: 26px;
    margin-right: 2px;
    overflow: hidden;
    padding: 0 6px;
}
.pages a, .pageback a {
