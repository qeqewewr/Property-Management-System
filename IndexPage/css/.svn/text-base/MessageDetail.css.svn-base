    padding: 0 14px;
    text-decoration: none;
}
#menu a.dropmenu {
    background-image: url("../../images/default/arrow_down.gif");
    background-position: 95% 50%;
    background-repeat: no-repeat;
    padding-right: 20px;
}
#menu a:hover {
    background-color: #1E4B7E;
    border-color: #00B2E8;
    color: #FFFFFF;
}
#menu li.current a {
    background-color: #1E4B7E;
    border-color: #00B2E8;
    color: #FFFFFF;
    height: 26px;
}
#nav {
    background: url("../../images/default/icon_nav.gif") no-repeat scroll 0 50% transparent;
    line-height: 3em;
    margin: -8px auto 0;
    text-align: left;
    text-indent: 25px;
}
#nav, #nav a {
    color: #FFFFFF;
}
.mainbox table {
    width: 100%;
}
.blocktitle {
    font-size: 16px;
    font-weight: 700;
    margin-bottom: 0.5em;
}
.channelinfo {
