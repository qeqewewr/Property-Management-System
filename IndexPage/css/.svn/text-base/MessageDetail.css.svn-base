}
#footer {
    padding: 1em 0;
}
#rightinfo {
    float: left;
}
#footlink {
    float: right;
    text-align: right;
}
#footer, #footer a {
    color: #8691A2;
}
.content {
    padding: 20px 25px;
}
.content h1 {
    font-size: 16px;
    font-weight: 400;
    margin-bottom: 10px;
}
#menu {
    bottom: 0;
    height: 26px;
    position: absolute;
    right: 0;
}
#menu li {
    background: url("../../images/default/mtabbg.gif") repeat-x scroll 0 100% #EBF4FD;
    display: inline;
    float: left;
    margin-left: 5px;
}
#menu a {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 transparent;
    border-color: #B0E4EF;
    border-style: solid;
    border-width: 1px 1px 0;
    color: #666666;
    float: left;
    height: 25px;
    line-height: 25px;
