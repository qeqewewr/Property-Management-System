    width: 22px;
}
.pages a.next {
    background-image: url("../../images/default/arrow_right.gif");
    background-position: 90% 50%;
    padding-right: 16px;
}
.pageback a {
    background: url("../../images/default/arrow_left.gif") no-repeat scroll 0 50% #FFFFFF;
    padding-left: 16px;
}
.pages a:hover, .pageback a, .pages a.next {
