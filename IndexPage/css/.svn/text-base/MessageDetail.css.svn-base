    width: 100%;
}
.promptmenu a:hover {
    color: #0099CC;
    text-decoration: none;
}
.up {
    background-position: -500px 100%;
    padding-bottom: 10px;
    padding-top: 0;
    width: 150px;
}
.up .promptcontent {
    background-position: -650px 0;
    padding-bottom: 0;
    padding-top: 5px;
}
.up li {
    border-bottom: medium none;
    line-height: 160%;
}
#uchome_ntc {
    background: url("../../images/default/notify_bg_m.gif") no-repeat scroll 0 50% transparent;
    color: #FFFFFF !important;
    padding: 1px 10px 3px 15px;
}
#uchome_ntc a {
    margin: 0;
}
#umenu, #umenu a, #umenu .pipe {
