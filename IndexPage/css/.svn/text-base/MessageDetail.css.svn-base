* {
    word-wrap: break-word;
}
html, body {
    border: 0 none !important;
}
body {
    background: none repeat scroll 0 0 #FFFFFF;
    text-align: center;
}
body, td, input, textarea, select, button {
    color: #444444;
    font: 12px/1.6em Verdana,Helvetica,Arial,sans-serif;
}
input, select, button {
    vertical-align: middle;
}
textarea {
    resize: none;
}
body, ul, li, dl, dd, p, h1, h2, h3, h4, h5, h6, form, fieldset {
    margin: 0;
    padding: 0;
}
h1, h2, h3, h4, h5, h6 {
    font-size: 1em;
}
ul li {
    list-style: none outside none;
}
a {
    color: #000000;
    text-decoration: none;
}
a:hover {
    text-decoration: underline;
}
.lightlink, .lightlink a {
    color: #0099CC;
}
a img {
    border: medium none;
}
em, cite, strong, th {
    font-style: normal;
    text-align: left;
}
.emfont {
    color: #FF6600;
}
table {
    border-collapse: collapse;
    empty-cells: show;
}
th {
    font-weight: 400;
}
.txt, .txtarea {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    background: none repeat scroll 0 0 #FFFFFF;
    border-color: #999999 #CCCCCC #CCCCCC #999999;
    border-style: solid;
    border-width: 1px;
    height: 16px;
    padding: 2px;
}
.txtarea {
    height: auto;
}
.checkbox, .radio {
    margin: 0;
    width: 15px;
}
.pipe {
    margin: 0 5px;
}
.lighttxt, .lighttxt a, .pipe {
    color: #999999;
}
.smalltext {
    font-family: Verdana,Helvetica,Arial,sans-serif;
    font-size: 0.83em;
}
.noborder, .noborder td {
    border: medium none !important;
}
.nobg {
    background: none repeat scroll 0 0 transparent !important;
}
.right {
    float: right;
}
.left {
    float: left;
}
.onerror, .onright {
    background: url("../../images/default/check_error.gif") no-repeat scroll 2px 2px transparent;
    clear: both;
    padding: 2px 2px 2px 22px;
}
.onright {
    background: url("../../images/default/check_right.gif") no-repeat scroll 2px 2px transparent;
}
.notice {
    background: url("../../images/default/notice.gif") no-repeat scroll 2px 6px transparent;
    clear: both;
    margin: 5px 0;
    padding: 3px 5px 3px 20px;
}
.nodata {
    color: #999999;
    font-size: 16px;
    font-weight: 700;
    margin: 99px auto;
    text-align: center;
}
#ajaxwaitid {
    background: none repeat scroll 0 0 #DD0000;
    color: #FFFFFF;
    display: none;
    height: 1.6em;
    line-height: 1.6em;
    overflow: hidden;
    position: absolute;
    right: 0;
    top: 0;
    width: 100px;
    z-index: 100;
}
#checksecanswer img, #checksecanswer3 img, #checkseccodeverify img, #checkseccodeverify3 img, .warning img {
    vertical-align: middle;
}
.s_clear:after {
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
}
.s_clear {
}
hr {
    background: none repeat scroll 0 0 transparent;
    border: 0 solid #E6E7E1;
    clear: both;
    margin: 1em 0;
}
.solidline {
    border-bottom-width: 1px;
    border-top: 1px solid #FFFFFF;
}
.dashline {
    border-bottom-width: 1px;
    border-style: dashed;
    height: 0;
}
.shadowline {
    border-bottom-color: #FFFFFF;
    border-top-color: #C8DCEC;
    border-width: 1px 0;
}
.side .shadowline {
    margin: 5px 0;
}
.cornerlayger {
    background: none repeat scroll 0 0 #EBF2F8;
    border: 1px solid #0099CC;
    line-height: 1.6em;
    padding: 6px 8px;
}
.minicorner {
    background: url("../../images/default/attachinfobg.gif") no-repeat scroll 50% 0 transparent;
    height: 6px;
    margin-top: -1px;
}
.wrap {
    margin: 0 auto;
    text-align: left;
}
#wrap {
    background-color: #FFFFFF;
    border: 0 solid;
    clear: both;
    min-height: 450px;
    padding-bottom: 10px;
}
* html #wrap {
    height: 450px;
}
.wrap, #nav {
    width: 960px;
}
#header {
    background: none repeat scroll 0 0 #FFFFFF;
    border-bottom: 0 solid #00B2E8;
    margin-bottom: 8px;
    min-width: 960px;
    width: 100%;
}
#header .wrap {
    padding: 24px 0 22px;
    position: relative;
}
#header h2 {
    float: left;
}
#umenu {
    line-height: 20px;
    position: absolute;
    right: 0;
    top: 0;
}
#umenu a {
    margin: 0 6px;
}
#umenu cite {
    font-weight: 700;
}
#umenu cite span a {
    font-weight: 400;
    margin: 0;
}
#myprompt.new {
    background: url("../../images/default/notice_newpm.gif") no-repeat scroll 0 50% transparent;
    padding-left: 22px;
}
#myprompt_menu a.new {
    color: #FF6600;
}
#myprompt_menu li {
    float: left;
    width: 50%;
}
.promptmenu {
    background: url("../../images/default/pm_ntc.gif") no-repeat scroll 0 0 transparent;
    margin-left: -27px;
    opacity: 0.98;
    padding-top: 10px;
    text-align: left;
    width: 250px;
}
.promptcontent {
    background: url("../../images/default/pm_ntc.gif") no-repeat scroll -250px 100% transparent;
    padding-bottom: 10px;
}
.promptmenu ul {
    -moz-border-bottom-colors: none;
    -moz-border-image: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
